<!-- Auto Check addressed checkbox -->
		<script type="text/javascript">
		function changeToChecked(val, elt){
			if(val){
				elt.checked= true;
			}
		}
		function onFocusTextArea(val, elt){
		elt.checked = false;

		}
		function onBlurTextArea(val, elt){
		if(elt.value!=""){
		elt.checked = true;
		}
		}
		</script>

