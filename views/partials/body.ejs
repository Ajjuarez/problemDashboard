<!-- views/partials/body.ejs  -->
<!-- This file contains the body of the application, it holds the menu bar, the data display
	and button accordion. Step 3. -->
    
    <!-- Interface Design begins here:-->
	<section class="interface">
		
		<!-- Colored square used to indicate size - remove after devel-->
		<div class="SIZE"></div>

		
		<!-- modals  -->
		<div id="modal1" class="modal">
			<div id="insert1"></div>
		</div>
		<div id="modal2" class="modal">
			<div id="insert2"></div>
		</div>
		<div id="modal3" class="modal">
			<div id="insert3"></div>
		</div>


		
		<!-- Search and display options at the top of the interface-->
		<!-- calling the display menu  -->
		<%- include ('menu') %>

		<!-- bottom panel -->
		<div id="displayAndAccordionContainer">
			<div id="dataDisplay">
                <!-- calling the datadisplay  -->
				<%- include ('dataDisplay') %>
			</div>
		</div>


		  
	</section>
	

